<%
  header_tags_content = content_for(:header_tags)

  if header_tags_content.present? && header_tags_content.include?('/jstoolbar/')
    unless header_tags_content.include?('redmine_jstoolbar_ext.js')
      content_for :header_tags do
        javascript_include_tag('redmine_jstoolbar_ext.js?20170601', plugin: 'redmine_jstoolbar_ext') + "\n" +
        stylesheet_link_tag('redmine_jstoolbar_ext.css?20170601', plugin: 'redmine_jstoolbar_ext') + "\n"
      end
    end

    content_for :header_tags do
      javascript_include_tag('redmine_jstoolbar_ext_buttons.js?20170601', plugin: 'redmine_jstoolbar_ext_buttons') + "\n" +
      stylesheet_link_tag('redmine_jstoolbar_ext_buttons.css?20170601', plugin: 'redmine_jstoolbar_ext_buttons') + "\n"
    end
  end
-%>
